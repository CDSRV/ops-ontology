#################################################################
#    Annotation properties
#################################################################

###  http://ontology/ops#assembly
:assembly rdf:type owl:AnnotationProperty ;
          rdfs:domain _:genid2147484120 .


#################################################################
#    Object Properties
#################################################################

###  http://ontology/ops#blockStorage
:blockStorage rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isStorage ;
              rdfs:domain :block .


###  http://ontology/ops#compileTarget
:compileTarget rdf:type owl:ObjectProperty ;
               rdfs:domain :codeType ;
               rdfs:range :codeType .


###  http://ontology/ops#hasCores
:hasCores rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isCompute ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :cores ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :CoreCount ;
                       owl:someValuesFrom xsd:short
                     ] .


###  http://ontology/ops#hasInstructionSetArchitecture
:hasInstructionSetArchitecture rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :isCompute .


###  http://ontology/ops#hasMemory
:hasMemory rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :isCompute ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :memory ;
           rdfs:range [ rdf:type owl:Restriction ;
                        owl:onProperty :MemorySize ;
                        owl:someValuesFrom xsd:short
                      ] .


###  http://ontology/ops#inProvider
:inProvider rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :hostingProvider ;
            rdfs:comment "digital infrastructure services"^^xsd:string .


###  http://ontology/ops#inProviderPartition
:inProviderPartition rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :inProvider .


